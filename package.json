{
  "name": "passport-mock-strategy",
  "version": "1.0.0",
  "description": "Mock passport strategy for testing Node.js applications.",
  "keywords": [
    "passport mock",
    "auth mock"
  ],
  "author": {
    "name": "Christopher Szatmary",
    "email": "cs@christopherszatmary.com",
    "url": "https://christopherszatmary.com"
  },
  "repository": "cszatma/passport-mock-strategy",
  "bugs": {
    "url": "https://github.com/cszatma/passport-mock-strategy/issues"
  },
  "license": "MIT",
  "main": "lib/index.js",
  "engines": {
    "node": ">= 6"
  },
  "files": [
    "lib/"
  ],
  "dependencies": {
    "passport": "^0.4.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.3",
    "babel-preset-env": "^1.6.1",
    "babel-preset-flow": "^6.23.0",
    "cookie-parser": "^1.4.3",
    "cookie-session": "^2.0.0-beta.3",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-flowtype": "^2.46.2",
    "eslint-plugin-prettier": "^2.6.0",
    "express": "^4.16.3",
    "flow-bin": "^0.69.0",
    "flow-copy-source": "^1.3.0",
    "husky": "^0.14.3",
    "jest": "^22.4.3",
    "jest-supertest-cookie-fix": "^1.0.0",
    "lint-staged": "^7.0.4",
    "prettier": "^1.11.1",
    "superagent": "^3.8.2",
    "supertest": "^3.0.0"
  },
  "scripts": {
    "build": "yarn run clean && yarn run build:lib && yarn run build:flow",
    "build:lib": "babel src -d lib",
    "build:flow": "flow-copy-source src lib",
    "clean": "rm -rf lib",
    "eslint": "eslint --fix \"{src,test}/**/*.js\"",
    "flow": "flow",
    "precommit": "lint-staged",
    "prepare": "yarn run build",
    "prettier": "prettier --write \"{src,test}/**/*.js\"",
    "test": "jest",
    "test-watch": "jest --watch"
  }
}
